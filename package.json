{
    "name": "js-problems",
    "version": "1.0.0",
    "scripts": {
        "client:local": "cd client && REACT_APP_ENV=local npm start",
        "client:prod": "cd client && REACT_APP_ENV=prod npm start",
        "server": "nodemon server/server/server.js",
        "start": "node server/server/server.js",
        "start:dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client:local\""
    },
    "dependencies": {
        "@google-cloud/storage": "^2.0.3",
        "aws-sdk": "^2.377.0",
        "bluebird": "^3.5.2",
        "body-parser": "^1.18.3",
        "busboy": "^0.2.14",
        "concurrently": "^3.5.0",
        "cors": "^2.8.5",
        "create-html": "^4.1.0",
        "ejs": "^2.6.1",
        "express": "^4.16.3",
        "express-handlebars": "^3.0.2",
        "google-auto-auth": "^0.10.1",
        "handlebars": "^4.1.2",
        "hbs": "^4.0.4",
        "mongo": "^0.1.0",
        "mongodb": "^3.1.10",
        "multer": "^1.3.1",
        "node-fetch": "^2.2.0",
        "path": "^0.12.7",
        "react-fullstory": "^1.0.3",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1"
    },
    "proxy": "http://localhost:5000/",
    "devDependencies": {
        "node-sass": "^4.12.0",
        "nodemon": "^1.18.4",
        "sass": "^1.14.0",
        "sass-loader": "^7.1.0"
    }
